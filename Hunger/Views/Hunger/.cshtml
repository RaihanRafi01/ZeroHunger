
@{
    ViewBag.Title = "SingUpEmp";
    Layout = "~/Views/Shared/_LayoutHunger.cshtml";
}

<h2>SignUP For Employee </h2>

<form method="post">
    <input type="text" name="Name" placeholder="Name" class="form-control" /><br />
    <input type="password" name="Password" placeholder="Password" class="form-control" /><br />
    <input type="text" name="Email" placeholder="Email" class="form-control" /><br />
    <input type="text" name="Contact" placeholder="Contact Number" class="form-control" /><br />
    <input type="submit" value="Join" class="btn btn-success" />
</form>

